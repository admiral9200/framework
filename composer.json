{
  "name": "shopper/framework",
  "description": "An eCommerce administration built with Laravel 6 for create online shop.",
  "keywords": [
    "laravel",
    "admin",
    "shopper",
    "e-commerce"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Monney",
      "email": "contact@arthurmonney.com",
      "homepage": "https://arthurmonney.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-intl": "*",
    "illuminate/support": "^7.0",
    "laravel/helpers": "^1.2",
    "laravel/sanctum": "^2.2",
    "laravelcollective/html": "^6.0",
    "lavary/laravel-menu": "^1.7",
    "livewire/livewire": "^1.0",
    "maatwebsite/laravel-sidebar": "~2.3",
    "mckenziearts/laravel-notify": "^1.1",
    "moneyphp/money": "^3.3",
    "spatie/laravel-permission": "^3.2",
    "torann/geoip": "^1.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "files": [
      "src/Helpers/helpers.php"
    ],
    "psr-4": {
      "Shopper\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shopper\\Framework\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shopper\\Framework\\FrameworkServiceProvider"
      ],
      "aliases": {
        "Shopper": "Shopper\\Framework\\Facades\\Shopper"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
